{
	"name": "vscode-language-applesoft",
	"displayName": "language-applesoft",
	"description": "Language support for Applesoft BASIC",
	"version": "0.0.1",
	"license": "GPL-3.0-only",
	"publisher": "dfgordon",
	"repository": {
		"type": "git",
		"url": "https://github.com/dfgordon/vscode-language-applesoft"
	},
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:applesoft"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "applesoft",
				"extensions": [
					".bas"
				]
			}
		]
	},
	"dependencies": {
		"tree-sitter": "^0.17",
		"tree-sitter-applesoft": "^1.0.6"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^14.16.0",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.4.3"
	}
}
